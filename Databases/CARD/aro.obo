[Term]
id: ARO:3004360
name: antimicrobial resistance testing reference standard
namespace: antibiotic_resistance
def: "Uses breakpoints to create standards for interpreting the clinical outcome of an MIC value for a particular microbial species and antimicrobial agent" []
